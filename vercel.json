{
  "build": {
    "env": {
      "SUPABASE_URL": "@supabase_url",
      "SUPABASE_ANON_KEY": "@supabase_anon_key"
    }
  },
  "functions": {
    "api/app.mjs": {
      "runtime": "nodejs20.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/app"
    },
    {
      "source": "/:code",
      "destination": "/api/app"
    }
  ]
}